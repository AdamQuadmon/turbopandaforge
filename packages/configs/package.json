{
  "name": "@turbopandaforge/configs",
  "version": "0.1.0",
  "description": "A package for managing configurations in Next.js projects.",
  "exports": {
    "./app": {
      "types": "./src/app/index.ts",
      "default": "./src/app/index.ts"
    },
    "./app/*": {
      "types": "./src/app/*.ts",
      "default": "./src/app/*.ts"
    },
    "./resolvers/*": {
      "types": "./src/resolvers/*.ts",
      "default": "./src/resolvers/*.ts"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules"
  },
  "devDependencies": {
    "@turbopandaforge/tsconfig": "workspace:*",
    "@turbopandaforge/types": "workspace:*",
    "@turbopandaforge/utils": "workspace:*",
    "consola": "3.2.3",
    "typescript": "^5.6.2",
    "c12": "^2.0.0"
  }
}
