{
  "name": "@turbopandaforge/nextjs",
  "version": "0.0.1",
  "description": "Next.js utilities",
  "exports": {
    "./generators/*": {
      "types": "./src/generators/*.ts",
      "default": "./src/generators/*.ts"
    },
    "./plugins/*": {
      "types": "./src/plugins/*.ts",
      "default": "./src/plugins/*.ts"
    },
    "./ui/*": {
      "types": "./src/ui/*.tsx",
      "default": "./src/ui/*.tsx"
    }
  },
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules"
  },
  "devDependencies": {
    "@million/lint": "^1.0.0",
    "@next/bundle-analyzer": "^14.2.14",
    "@turbopandaforge/tsconfig": "workspace:*",
    "@turbopandaforge/types": "workspace:*",
    "next": "14.2.14",
    "react": "18.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
